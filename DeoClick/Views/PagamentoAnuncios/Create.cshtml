@model DeoClick.Models.PagamentoAnuncio

@{
    ViewData["Title"] = "Cadastrar";
}

<div>
    <a asp-action="Index">Voltar</a>
</div>
<br />

<h1 style="color:blue;">Cadastrar Um Tipo de Pagamento para o Anúncio</h1>
<br />
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="idFormaPagamento" class="control-label"></label>
                <select asp-for="idFormaPagamento" class="form-control" asp-items="ViewBag.idFormaPagamento"></select>
                <span asp-validation-for="idFormaPagamento" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
